extends base 

block content 
    .account
        .account-main 
            h2 Account
            span 
                span= user.username
                | 
                span ,
                | 
                span= user.email
                | 
                span . 
                | 
                a(href="#") Go to profile
        .account-card
            .account-card__first.cardcover
                a(href="/me/personal")
                    .account-card__first.a
                        img(src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcTXx3dHQqME_0Zeh16fZUMuLqN9rgPQazPEzE_xEEQ3Von-Jj4c&usqp=CAU", alt="user svg")
                    .account-card__first.b
                        h4 Peronal Info >
                    .account-card__first.c
                        span Provide personal details and how we can reach you
            .account-card__first.cardcover
                a(href="/me/login-security")    
                    .account-card__first.a
                        img(src="data:image/png;base64,", alt="security")
                    .account-card__first.b
                        h4 Login and Security >
                    .account-card__first.c
                        span Update your password and secure your account
            .account-card__first.cardcover
                a(href="/me/my-hotel") 
                    .account-card__first.a
                        img(src="https://cdn3.iconfinder.com/data/icons/hotel-112/50/32-512.png", alt="security")
                    .account-card__first.b
                        h4 My Booked Hotel >
                    .account-card__first.c
                        span Just Check Your Booked Hotel 

        .account-delete
            span Need to deactivate your account
            br
            a(href="#") Take care of that now
