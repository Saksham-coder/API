extends base

block content   
  
  main.mainsign
    .left
      //- img(src="https://previews.123rf.com/images/oneinchpunch/oneinchpunch1811/oneinchpunch181100465/111679584-group-of-friends-making-barbecue-in-the-nature-happy-people-having-fun-on-a-picnic-in-the-countrysid.jpg", alt="")
      .left-text
        h1
          | Explore with 
          br
          |  friends.
    .right
      .signup-form
        .signup-form_text
          img.signup_logo(src="data:image/png;base64,", alt="logo")
          h2.heading-secondary.ma-bt-lg Register 
          p
            | Already have an account ? 
            a(href='/login') Log In here
        form.signform.formdesign
          .form__group
            label.form__label.form__label__text(for='email') Email Address
            br
            input#email.form__input(type='email' required)
          .form__group.ma-bt-md
            label.form__label.form__label__text(for='username') Username
            br
            input#username.form__input(type='text' required)
          .form__group.ma-bt-md
            label.form__label.form__label__text(for='password') Password
            br
            input#password.form__input(type='password' required)
          .form__group.ma-bt-md
            label.form__label.form__label__text(for='confirmPassword') Confirm Password
            br
            input#confirmPassword.form__input(type='password' required)    
          .form__group
            button.btn.btn--green Sign Up  